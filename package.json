{
  "name": "yet-anothre-speedcube-chart",
  "version": "0.0.0",
  "description": "This app shows a graph based on the .csv  exported from csTimer.",
  "scripts": {
    "clean": "rimraf /dist",
    "dev": "esbuild src/index.tsx --watch --bundle --sourcemap --outfile=docs/index.js --define:process.env.NODE_ENV=\\\"development\\\"",
    "build": "npm run clean | esbuild src/index.tsx --bundle --metafile=metafile.json --minify --sourcemap --outfile=docs/index.js --define:process.env.NODE_ENV=\\\"production\\\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "speedcube"
  ],
  "author": "uminus",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.9.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@sgratzl/chartjs-chart-boxplot": "^3.0.0-rc.1",
    "chart.js": "3.0.0-rc.6",
    "preact": "^10.5.13"
  }
}
