{
  "name": "yet-anothre-speedcube-chart",
  "version": "0.0.0",
  "description": "This app shows a graph based on the .csv  exported from csTimer.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf /dist",
    "dev": "esbuild app.tsx --watch --bundle --sourcemap --outfile=docs/index.js --define:process.env.NODE_ENV=\\\"development\\\"",
    "build": "npm run clean | esbuild app.tsx --bundle --minify --sourcemap --outfile=docs/index.js --define:process.env.NODE_ENV=\\\"production\\\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "speedcube"
  ],
  "author": "uminus",
  "license": "MIT",
  "devDependencies": {
    "@types/chart.js": "^2.9.31",
    "esbuild": "^0.9.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "chart.js": "^2.9.4",
    "preact": "^10.5.13"
  }
}
